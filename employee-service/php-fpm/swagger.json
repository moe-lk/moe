          },
          "404": {
            "description": "User not found"
          }
        },
        "security": [
          {
            "cs_auth": [
              "write:user",
              "read:user"
            ]
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          },
          "description": "Updated user object",
          "required": true
        }
      }
    },
    "/user": {
